{
  "name": "@snomiao/userscript",
  "version": "1.2.3",
  "description": "a collection of snos userscripts",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "readme": "docs/README.md",
  "scripts": {
    "serve:docs": "npx docsify-cli serve docs",
    "serve": "npx serve .",
    "build": "node --experimental-specifier-resolution=node --experimental-modules --no-warnings ./builder/build.mjs",
    "format": "npx -y -- prettier --write .",
    "prerelease": "npm run build && git add .",
    "release": "standard-version",
    "postrelease": "git push --follow-tag --all",
    "test": "",
    "subtree": "cmd /c start \"\" subtree",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snomiao/userscript.js.git"
  },
  "author": "snomiao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/snomiao/userscript.js/issues"
  },
  "homepage": "https://snomiao.github.io/userscript.js/",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "globby": "^13.1.1",
    "html-escaper": "^3.0.3",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.2",
    "minify": "^8.0.3",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.2",
    "terser": "^5.10.0",
    "userscript-meta": "^1.0.1"
  },
  "dependencies": {
    "bookmarklet": "^3.0.0"
  }
}
